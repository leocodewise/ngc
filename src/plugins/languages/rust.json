{
    "tokens": [
      {
        "type": "KEYWORD",
        "patterns": [
          "fn", "let", "mut", "const", "if", "else", "match", "loop", "while", "for", "in", "break", "continue", "return",
          "struct", "enum", "impl", "trait", "pub", "use", "mod", "as", "self", "super", "crate", "where", "type", "dyn"
        ]
      },
      {
        "type": "IDENTIFIER",
        "pattern": "[a-zA-Z_][a-zA-Z0-9_]*"
      },
      {
        "type": "NUMBER",
        "pattern": "[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?"
      },
      {
        "type": "OPERATOR",
        "patterns": [
          "+", "-", "*", "/", "%", "==", "!=", "<=", ">=", "<", ">", "=", "=>", "->", "|", "&", "^", "!", "&&", "||", "..", "..="
        ]
      },
      {
        "type": "PUNCTUATION",
        "patterns": [
          ";", "{", "}", "(", ")", "[", "]", ",", ":", "::", ".", "->", "?", "@"
        ]
      },
      {
        "type": "STRING",
        "pattern": "\"[^\"]*\"|r#*\".*?\"#*"
      },
      {
        "type": "CHAR",
        "pattern": "'[^']'"
      },
      {
        "type": "COMMENT",
        "pattern": "//.*|/\\*[\\s\\S]*?\\*/"
      },
      {
        "type": "MACRO",
        "pattern": "#[a-zA-Z_][a-zA-Z0-9_]*!?"
      },
      {
        "type": "LIFETIME",
        "pattern": "'[a-zA-Z_][a-zA-Z0-9_]*"
      }
    ]
  }